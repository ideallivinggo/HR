<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.aaa.rzhr.dao.RzmessageMapper">
    <insert id="addRzmessageX" parameterType="Rzmessage">
                  INSERT INTO rzmessage(metext,begintime,endtime,mestate,metitle) VALUE(#{metext},#{begintime},#{endtime},#{mestate},#{metitle})
    </insert>


</mapper>